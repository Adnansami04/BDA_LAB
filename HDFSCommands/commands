start-all.sh

jps

1. mkdir
hdfs dfs -mkdir /adnan

2. ls
hadoop fs -ls /

3. put
hdfs dfs -put /home/hduser/welcome.txt /adnan/wc.txt

4. copyFromLocal
hdfs dfs -copyFromLocal /home/hduser/welcome.txt /daya/wc2.txt

5. get
  i: hdfs dfs -get /anant/wc.txt /home/hduser/Downloads/WAWC.txt
  ii: hdfs dfs -getmerge /adnan/wc.txt /adnan/wc1.txt /home/hduser/Desktop/Merge.txt
  iii:hadoop fs -getfacl /adnan/
  
6. copyToLocal
hdfs dfs -copyToLocal /anant/wc.txt /home/hduser/Desktop

7. cat
hdfs dfs -cat /adnan/wc.txt

8. mv
hadoop fs -mv /adnan /daya

9. cp
hadoop fs -cp /daya /adnan



